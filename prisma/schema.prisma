generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id              Int     @id @default(autoincrement())
  uu_id           String  @default(uuid())
  avatar          String
  name            String
  email           String  @unique
  password        String
  description     String
  riot_id         String
  lane            String
  riot_nick_name  String


  @@map("users")
}

